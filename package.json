{
  "name": "ksschoerke",
  "version": "1.0.0",
  "description": "A blank template to get started with Payload 3.0",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "cross-env NODE_OPTIONS=--no-deprecation next build",
    "dev": "cross-env NODE_OPTIONS=--no-deprecation next dev",
    "devsafe": "rm -rf .next && cross-env NODE_OPTIONS=--no-deprecation next dev",
    "format": "pnpm exec prettier . --write",
    "generate:importmap": "cross-env NODE_OPTIONS=--no-deprecation payload generate:importmap",
    "generate:media": "tsx scripts/utils/regenerateMediaSizes.ts",
    "generate:types": "cross-env NODE_OPTIONS=--no-deprecation payload generate:types",
    "lint": "cross-env NODE_OPTIONS=--no-deprecation next lint",
    "migrate": "cross-env NODE_OPTIONS=--no-deprecation payload migrate",
    "migrate:create": "cross-env NODE_OPTIONS=--no-deprecation payload migrate:create",
    "migrate:media": "cross-env NODE_OPTIONS=--no-deprecation payload run scripts/wordpress/migrateMedia.js",
    "migrate:discography": "tsx scripts/migrateDiscographyToRecordings.ts",
    "payload": "cross-env NODE_OPTIONS=--no-deprecation payload",
    "start": "cross-env NODE_OPTIONS=--no-deprecation next start",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "seed:all": "tsx scripts/db/seedAll.ts",
    "seed:artists": "payload run ./scripts/db/seedArtists.ts",
    "seed:media": "payload run ./scripts/db/seedMedia.ts",
    "seed:static-pages": "tsx scripts/db/seedStaticPages.ts",
    "restore:artists-dump": "tsx scripts/db/restoreArtistsDump.ts",
    "restore:posts-dump": "tsx scripts/db/restorePostsDump.ts",
    "dump": "tsx scripts/db/dumpCollection.ts",
    "dump:artists": "pnpm dump artists",
    "dump:employees": "pnpm dump employees",
    "dump:recordings": "pnpm dump recordings",
    "migrate:employees": "payload run scripts/wordpress/migrateEmployees.ts"
  },
  "packageManager": "pnpm@10.22.0",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.932.0",
    "@libsql/client": "^0.15.15",
    "@payloadcms/db-sqlite": "^3.64.0",
    "@payloadcms/next": "^3.64.0",
    "@payloadcms/payload-cloud": "^3.64.0",
    "@payloadcms/plugin-search": "^3.64.0",
    "@payloadcms/richtext-lexical": "^3.64.0",
    "@payloadcms/storage-s3": "^3.64.0",
    "@payloadcms/translations": "^3.64.0",
    "@payloadcms/ui": "^3.64.0",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-toggle": "^1.1.10",
    "@radix-ui/react-toggle-group": "^1.1.11",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cross-env": "^10.0.0",
    "embla-carousel-react": "^8.6.0",
    "fast-xml-parser": "^5.3.2",
    "graphql": "^16.12.0",
    "kbar": "0.1.0-beta.48",
    "libsql": "^0.5.22",
    "lodash": "^4.17.21",
    "lucide-react": "^0.553.0",
    "mime": "^4.0.7",
    "next": "16.0.3",
    "next-intl": "^4.5.3",
    "payload": "^3.64.0",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "sharp": "0.34.5",
    "tailwind-merge": "^3.4.0",
    "yaml": "^2.8.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.39.1",
    "@tailwindcss/postcss": "^4.1.17",
    "@tailwindcss/typography": "^0.5.16",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/lodash": "^4.17.20",
    "@types/node": "^24.10.1",
    "@types/react": "npm:types-react@19.0.0-rc.1",
    "@types/react-dom": "npm:types-react-dom@19.0.0-rc.1",
    "@typescript-eslint/eslint-plugin": "^8.46.4",
    "@vitejs/plugin-react": "^5.1.1",
    "@vitest/ui": "^4.0.13",
    "dotenv": "^17.2.3",
    "eslint": "^9.39.1",
    "eslint-config-next": "16.0.3",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-perfectionist": "^4.15.0",
    "happy-dom": "^20.0.10",
    "jsdom": "^27.2.0",
    "postcss": "^8.5.6",
    "prettier": "3.6.2",
    "prettier-plugin-organize-imports": "^4.2.0",
    "prettier-plugin-tailwindcss": "^0.7.1",
    "tailwindcss": "^4.1.17",
    "tailwindcss-animate": "^1.0.7",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.6",
    "typescript": "5.9.3",
    "vitest": "^4.0.13"
  },
  "engines": {
    "node": "^18.20.2 || >=20.9.0"
  },
  "pnpm": {
    "overrides": {
      "@types/react": "npm:types-react@19.0.0-rc.1",
      "@types/react-dom": "npm:types-react-dom@19.0.0-rc.1"
    }
  },
  "overrides": {
    "@types/react": "npm:types-react@19.0.0-rc.1",
    "@types/react-dom": "npm:types-react-dom@19.0.0-rc.1"
  }
}
