{
  "name": "ksschoerke",
  "version": "1.0.0",
  "description": "A blank template to get started with Payload 3.0",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "pnpm generate:search-index && cross-env NODE_OPTIONS=--no-deprecation next build --webpack",
    "dev": "cross-env NODE_OPTIONS=--no-deprecation next dev --webpack",
    "devsafe": "rm -rf .next && cross-env NODE_OPTIONS=--no-deprecation next dev",
    "format": "pnpm exec prettier . --write",
    "generate:importmap": "cross-env NODE_OPTIONS=--no-deprecation payload generate:importmap",
    "generate:media": "tsx scripts/utils/regenerateMediaSizes.ts",
    "generate:search-index": "cross-env NODE_OPTIONS=--no-deprecation payload run scripts/reindexSearch.ts",
    "generate:types": "cross-env NODE_OPTIONS=--no-deprecation payload generate:types",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit",
    "migrate": "cross-env NODE_OPTIONS=--no-deprecation payload migrate",
    "migrate:artists": "payload run scripts/wordpress/migrateArtists.ts",
    "migrate:employees": "payload run scripts/wordpress/migrateEmployees.ts",
    "migrate:create": "cross-env NODE_OPTIONS=--no-deprecation payload migrate:create",
    "migrate:media": "cross-env NODE_OPTIONS=--no-deprecation payload run scripts/wordpress/migrateMedia.js",
    "migrate:discography": "tsx scripts/migrateDiscographyToRecordings.ts",
    "payload": "cross-env NODE_OPTIONS=--no-deprecation payload",
    "start": "cross-env NODE_OPTIONS=--no-deprecation next start",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "dump": "tsx scripts/db/dumpCollection.ts",
    "dump:artists": "pnpm dump artists",
    "dump:employees": "pnpm dump employees",
    "dump:recordings": "pnpm dump recordings"
  },
  "packageManager": "pnpm@10.22.0",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.952.0",
    "@libsql/client": "^0.15.15",
    "@payloadcms/db-sqlite": "^3.68.3",
    "@payloadcms/email-resend": "^3.68.3",
    "@payloadcms/next": "^3.68.3",
    "@payloadcms/payload-cloud": "^3.68.3",
    "@payloadcms/plugin-search": "^3.68.3",
    "@payloadcms/richtext-lexical": "^3.68.3",
    "@payloadcms/storage-s3": "^3.68.3",
    "@payloadcms/storage-vercel-blob": "^3.68.3",
    "@payloadcms/translations": "^3.68.3",
    "@payloadcms/ui": "^3.68.3",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-toggle": "^1.1.10",
    "@radix-ui/react-toggle-group": "^1.1.11",
    "@vercel/blob": "^2.0.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cross-env": "^10.0.0",
    "embla-carousel-autoplay": "^8.6.0",
    "embla-carousel-react": "^8.6.0",
    "fast-xml-parser": "^5.3.3",
    "graphql": "^16.12.0",
    "kbar": "0.1.0-beta.48",
    "libsql": "^0.5.22",
    "lodash": "^4.17.21",
    "lucide-react": "^0.561.0",
    "mime": "^4.0.7",
    "next": "16.0.10",
    "next-intl": "^4.5.8",
    "payload": "^3.68.3",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "resend": "^6.6.0",
    "server-only": "^0.0.1",
    "sharp": "0.34.5",
    "tailwind-merge": "^3.4.0",
    "yaml": "^2.8.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.39.1",
    "@tailwindcss/postcss": "^4.1.18",
    "@tailwindcss/typography": "^0.5.16",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/lodash": "^4.17.20",
    "@types/node": "^25.0.1",
    "@types/react": "npm:types-react@19.0.0-rc.1",
    "@types/react-dom": "npm:types-react-dom@19.0.0-rc.1",
    "@typescript-eslint/eslint-plugin": "^8.49.0",
    "@vitejs/plugin-react": "^5.1.2",
    "@vitest/coverage-v8": "^4.0.15",
    "@vitest/ui": "^4.0.15",
    "dotenv": "^17.2.3",
    "eslint": "^9.39.1",
    "eslint-config-next": "16.0.10",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-perfectionist": "^4.15.0",
    "happy-dom": "^20.0.10",
    "postcss": "^8.5.6",
    "prettier": "3.7.4",
    "prettier-plugin-organize-imports": "^4.2.0",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "tailwindcss": "^4.1.18",
    "tailwindcss-animate": "^1.0.7",
    "ts-node": "^10.9.2",
    "tsx": "^4.21.0",
    "typescript": "5.9.3",
    "vitest": "^4.0.15"
  },
  "engines": {
    "node": "22.x"
  },
  "pnpm": {
    "overrides": {
      "@types/react": "npm:types-react@19.0.0-rc.1",
      "@types/react-dom": "npm:types-react-dom@19.0.0-rc.1",
      "dompurify": ">=3.2.4",
      "baseline-browser-mapping": "2.9.0-beta2"
    }
  },
  "overrides": {
    "@types/react": "npm:types-react@19.0.0-rc.1",
    "@types/react-dom": "npm:types-react-dom@19.0.0-rc.1",
    "dompurify": ">=3.2.4",
    "baseline-browser-mapping": "2.9.0-beta2"
  }
}
