{
  "name": "ksschoerke",
  "version": "1.0.0",
  "description": "A blank template to get started with Payload 3.0",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "cross-env NODE_OPTIONS=--no-deprecation next build",
    "dev": "cross-env NODE_OPTIONS=--no-deprecation next dev",
    "devsafe": "rm -rf .next && cross-env NODE_OPTIONS=--no-deprecation next dev",
    "format": "pnpm exec prettier . --write",
    "generate:importmap": "cross-env NODE_OPTIONS=--no-deprecation payload generate:importmap",
    "generate:types": "cross-env NODE_OPTIONS=--no-deprecation payload generate:types",
    "lint": "cross-env NODE_OPTIONS=--no-deprecation next lint",
    "migrate:media": "cross-env NODE_OPTIONS=--no-deprecation payload run scripts/migrateWordpressMedia.js",
    "payload": "cross-env NODE_OPTIONS=--no-deprecation payload",
    "start": "cross-env NODE_OPTIONS=--no-deprecation next start",
    "seed:artists": "payload run ./scripts/seedArtists.ts",
    "seed:employees": "payload run ./scripts/seedEmployees.ts",
    "dump": "tsx scripts/dumpCollection.ts",
    "dump:artists": "pnpm dump artists",
    "dump:employees": "pnpm dump employees"
  },
  "packageManager": "pnpm@10.19.0",
  "dependencies": {
    "@libsql/client": "^0.15.15",
    "@payloadcms/db-sqlite": "^3.61.1",
    "@payloadcms/next": "3.61.1",
    "@payloadcms/payload-cloud": "3.61.1",
    "@payloadcms/richtext-lexical": "3.61.1",
    "@payloadcms/storage-s3": "3.61.1",
    "@payloadcms/translations": "latest",
    "@payloadcms/ui": "^3.61.1",
    "@radix-ui/react-toggle": "^1.1.10",
    "@radix-ui/react-toggle-group": "^1.1.11",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cross-env": "^10.0.0",
    "embla-carousel-react": "^8.6.0",
    "fast-xml-parser": "^5.2.5",
    "graphql": "^16.11.0",
    "libsql": "^0.5.22",
    "lodash": "^4.17.21",
    "lucide-react": "^0.553.0",
    "mime": "^4.0.7",
    "next": "15.5.2",
    "payload": "3.61.1",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "sharp": "0.34.4",
    "tailwind-merge": "^3.4.0",
    "yaml": "^2.8.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.34.0",
    "@tailwindcss/postcss": "^4.1.12",
    "@tailwindcss/typography": "^0.5.16",
    "@types/lodash": "^4.17.20",
    "@types/node": "^24.3.0",
    "@types/react": "npm:types-react@19.0.0-rc.1",
    "@types/react-dom": "npm:types-react-dom@19.0.0-rc.1",
    "@typescript-eslint/eslint-plugin": "^8.41.0",
    "dotenv": "^17.2.3",
    "eslint": "^9.34.0",
    "eslint-config-next": "16.0.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-perfectionist": "^4.15.0",
    "postcss": "^8.5.6",
    "prettier": "3.6.2",
    "prettier-plugin-organize-imports": "^4.2.0",
    "prettier-plugin-tailwindcss": "^0.7.1",
    "tailwindcss": "^4.1.12",
    "tailwindcss-animate": "^1.0.7",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.6",
    "typescript": "5.9.3"
  },
  "engines": {
    "node": "^18.20.2 || >=20.9.0"
  },
  "pnpm": {
    "overrides": {
      "@types/react": "npm:types-react@19.0.0-rc.1",
      "@types/react-dom": "npm:types-react-dom@19.0.0-rc.1"
    }
  },
  "overrides": {
    "@types/react": "npm:types-react@19.0.0-rc.1",
    "@types/react-dom": "npm:types-react-dom@19.0.0-rc.1"
  }
}
